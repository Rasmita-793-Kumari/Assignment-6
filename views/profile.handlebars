{{> header2}}
<style>
  .gradient-custom {
    background: black;
   
  }
</style>

<section class="vh-100" style="background-color: rgb(136, 191, 245)">
  <div class="container p-3">
    <div class="row d-flex justify-content-center align-items-center h-100">
      <div class="col col-lg-6 mb-4 mb-lg-0">
        {{!-- <div class="card" style="back');border-radius: .5rem;"> --}}
          <div class="row g-0">
            <div class="col-md-8">
              <div class="card-body p-4">
                <h6 class="text-dark">{{data.name}}'s Profile</h6>
                <hr class="mt-0 mb-4">
                <div class="row pt-1">
                  <div class="col-6 mb-3">
                    <h6 style="color: rgb(26, 22, 22);"><u>Email</u></h6>
                    <p class="text-dark">{{data.email}}</p>
                  </div>
                  <br>
                  <div class="col-6 mb-3">
                    <h6 style="color: rgb(14, 12, 12);"><u>Phone</u></h6>
                    <p class="text-dark">{{data.mobile}}</p>
                  </div>
                </div>
                <h6 style="color: rgb(21, 20, 20);"><u>Details</u></h6>
                <hr class="mt-0 mb-4">
                <div class="row pt-1">
                  <div class="col-6 mb-3">
                    <h6 style="color: rgb(26, 22, 22);"><u>Address</u></h6>
                    <p class="text-dark">{{data.address}}</p>
                  </div>
                </div>
               
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  {{>footer}}
</section>